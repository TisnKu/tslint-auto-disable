{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "interface-name": true,
        "object-literal-sort-keys": false,
        "no-console": false,
        "no-shadowed-variable": false,
        "max-line-length": false,
        "triple-equals": [true, "allow-undefined-check"]
    }
}
