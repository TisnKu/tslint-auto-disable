{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "interface-name": false,
        "object-literal-sort-keys": false,
        "no-console": false,
        "no-shadowed-variable": false,
        "max-line-length": false,
        "triple-equals": [true, "allow-undefined-check"],
        "trailing-comma": false,
        "arrow-parens": false
    }
}
